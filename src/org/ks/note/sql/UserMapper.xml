<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"      
 "http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">
<!-- namespace 命名空间-->
<mapper namespace="org.ks.note.dao.UserDao">
<insert id="save" parameterType="org.ks.note.entity.User">
	insert into cn_user 
	    (cn_user_id,cn_user_name,cn_user_password,cn_user_token,cn_user_desc)
	    values(#{cn_user_id},#{cn_user_name},#{cn_user_password},#{cn_user_token},#{cn_user_desc})
</insert>

<select id="findByName" parameterType="string" resultType="org.ks.note.entity.User">
	select * from cn_user where cn_user_name=#{name}

</select>


</mapper>


